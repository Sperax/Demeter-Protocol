{
  "name": "demeter-protocol",
  "version": "1.0.0",
  "description": "Demeter protocol allows anyone to launch an incentivised liquidity mining campaign on top of Uniswap v3",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "install-pip-packages": "pip3 install -r requirements.txt",
    "initialize-pre-commit": "pre-commit install -c ./dev-utils/.pre-commit-config.yaml",
    "prepare": "npm-run-all install-pip-packages initialize-pre-commit",
    "solhint": "solhint -f table -c ./dev-utils/.solhint.json",
    "prettier:solidity": "prettier --write **/*.sol"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.7.3",
    "@openzeppelin/contracts-upgradeable": "^4.7.3",
    "@uniswap/v3-periphery": "^1.3.0",
    "@uniswap/v3-core": "^1.0.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "ethers": "^5.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sperax/Demeter-Protocol.git"
  },
  "keywords": [
    "YieldFarming",
    "UniswapV3",
    "SPA",
    "LP"
  ],
  "author": "Sperax Inc",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sperax/Demeter-Protocol/issues"
  },
  "homepage": "https://github.com/Sperax/Demeter-Protocol#readme"
}
